    %link{:href => "/bower_components/font-awesome/css/font-awesome.min.css", :rel => "stylesheet"}
    %link{:href => "/assets/css/style.css", :rel => "stylesheet"}
    %link{:href => "/assets/css/main.css", :rel => "stylesheet"}
    %link{:href => "/assets/css/to-top.css", :rel => "stylesheet"}
    %script{:src => "/bower_components/jquery/dist/jquery.min.js"}
    %script{:src => "/bower_components/bootstrap/dist/js/bootstrap.min.js"}
    %script{:src => "/assets/js/to-top.js"}
    %script{:src => "/assets/js/get-repo.js"}
    %meta{:content => "width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no", :name => "viewport"}/
    %meta{:content => "5-Oc6FC6uhyZawiHpuki_x09iG5cJ1cvtb19ScgNRp4", :name => "google-site-verification"}/
    :javascript
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
      ga('create', 'UA-73752623-2', 'auto');
      ga('send', 'pageview');
  %body
    .container
      .jumbotron
        .container
          %h1 Hiroto-K.github.io
          %p
            This is a web pages of
            = succeed "." do
              %a{:href => "https://github.com/Hiroto-K"} Hiroto-K
          .github-main
            %h2
              Public Repositories
              %span.label.label-primary.repo-count
            .list-group.repo-list
            %h2 GitHub Pages
            .list-group.pages-list
          .github-error
            %h2 Public Repositories
            %h3 Error
            %p.text-warning Sorry, An error has occurred.
          %h2 Links
          .list-group
            %a.list-group-item{:href => "https://hiroto-k.sakura.ne.jp/"} Home Page
            %a.list-group-item{:href => "https://hiroto-k.sakura.ne.jp/profile"} Profile
            %a.list-group-item{:href => "https://hiroto-k.sakura.ne.jp/work"} Work
            %a.list-group-item{:href => "https://hiroto-k.sakura.ne.jp/blog/"} Blog
            %a.list-group-item{:href => "https://hiroto-k.sakura.ne.jp/contact"} Contact
      .to-top
        %p.to-top-button
          %a.fa.fa-angle-up.fa-lg{:href => "#"}